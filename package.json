{
  "name": "watchers",
  "version": "1.0.0",
  "description": "",
  "main": "babel.config.js",
  "scripts": {
    "clear": "rm -rf ./build/* && rm -rf ./lib/*",
    "compile": "babel webpack-configs -d lib",
    "prepare": "npm run clear && npm run compile",
    "build-dev-browser": "webpack --config lib/browser.development.js",
    "build-dev-server": "webpack --config lib/server.development.js",
    "build-prod-browser": "webpack --config lib/browser.production.js",
    "build-prod-server": "webpack --config lib/server.production.js",
    "build-dev": "export NODE_ENV=development npm run prepare && npm run build-dev-browser && npm run build-dev-server",
    "build-prod": "export NODE_ENV=production npm run prepare && npm run build-prod-browser && npm run build-prod-server",
    "dev": "npm run build-dev && npm run start",
    "prod": "npm run build-prod && npm run start",
    "start": "node ./build/server.js",
    "heroku-postbuild": "export NODE_ENV=production && npm run build-prod-browser && npm run build-prod-server"
  },
  "author": "db",
  "license": "WTFPL",
  "dependencies": {
    "@babel/core": "^7.4.0",
    "@babel/polyfill": "^7.4.0",
    "autoprefixer": "^9.5.0",
    "classnames": "^2.2.6",
    "dotenv": "^7.0.0",
    "hogan.js": "^3.0.2",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-favicon": "^2.0.1",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "lodash.reduce": "^4.6.0",
    "mime": "^2.4.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^4.3.1",
    "request": "^2.88.0",
    "stylus": "^0.54.5",
    "webpack": "^4.29.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-add-module-exports": "^1.0.0",
    "colors": "^1.3.3",
    "compression-webpack-plugin": "^2.0.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.15.3",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-node": "^0.3.2",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "mini-css-extract-plugin": "^0.6.0",
    "mustache-loader": "^1.4.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "style-loader": "^0.23.1",
    "stylus-loader": "^3.0.2",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  }
}
